Stats for document type:
update alldocs_exceptlibrary_04May18 set category = ".doc" where locate(".doc",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".docx" where locate(".docx",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".pdf" where locate(".pdf",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".odt" where locate(".odt",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".xls" where locate(".xls",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".xlsx" where locate(".xlsx",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".ods" where locate(".ods",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".odt" where locate(".odt",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".ppt" where locate(".ppt",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".pptx" where locate(".pptx",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".txt" where locate(".txt",document_url) <> 0;
update alldocs_exceptlibrary_04May18 set category = ".rtf" where locate(".rtf",document_url) <> 0;

hosted on docviewer:
update alldocs_exceptlibrary_04May18 set docviewer = 1 where locate("docviewer.intelluslearning.com",document_url)<>0;

hosted on user-content-upload-prod:
update alldocs_exceptlibrary_04May18 set user_content_upload_prod = 1 where locate("user-content-upload-prod",document_url)<>0;



