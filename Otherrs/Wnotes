insert into doc_url_template values("","APUS","{PREFIX}","http://ebookcentral.proquest.com/lib/apus/reader.action?docID=","","NewSourceID");
insert into doc_url_template values("","BROWARD","{PREFIX}","http://ebookcentral.proquest.com/lib/broward-ebooks/reader.action?docID=","","NewSourceID");
