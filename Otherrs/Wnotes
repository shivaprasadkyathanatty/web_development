select c.course_id,c.course_name,c.deleted,c.global_course_discipline_classifier_id,c.global_course_source_id,cast(AES_DECRYPT(au.first_name,'7ZHsfXDRb7NIKv0jYpRmUO9G2X5HRnyrbP944uM5h') as CHAR(10000) character set utf8) coursecreator_firstname,count(dc.doc_seq_id) from ace_api_server_oer_student_exp.course c join ace_api_server_oer_student_exp.arp_user au on au.user_id=c.arp_owner_user_id join ace_api_server_oer_student_exp.course_lesson cl on cl.course_id=c.course_id join ace_api_server_oer_student_exp.learning_path lpd on cl.course_lesson_id=lpd.course_lesson_id join ace_api_server_oer_student_exp.learning_path_item lpi on lpd.learning_path_id=lpi.learning_path_id join centralized_content.document dc on lpi.doc_seq_id=dc.doc_seq_id where c.deleted=0 and au.deleted=0 and dc.deleted=0 and c.course_id=1314 group by c.course_id;

ace_api_server_oer_student_exp.learning_path_item lpi join ace_api_server_oer_student_exp.learning_path lpd on lpd.learning_path_id=lpi.learning_path_id join ace_api_server_oer_student_exp.course_lesson cl on cl.course_lesson_id=lpd.course_lesson_id join centralized_content.document dc on lpi.doc_seq_id=dc.doc_seq_id where c.deleted=0 and au.deleted=0 limit 1\G;

ace_api_server_gcmp_student_exp.learning_path_item lpi join ace_api_server_gcmp_student_exp.learning_path lpd on lpd.learning_path_id=lpi.learning_path_id join ace_api_server_gcmp_student_exp.course_lesson cl on cl.course_lesson_id=lpd.course_lesson_id join centralized_content.document dc on lpi.doc_seq_id=dc.doc_seq_id join ace_api_server_gcmp_student_exp.course c on c.course_id=cl.course_id where dc.deleted=0 and lpi.deleted=0 and c.course_id=1019 group by 1;

