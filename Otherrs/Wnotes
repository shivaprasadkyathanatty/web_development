select course_code,dc.doc_seq_id from ace_api_server_oer_student_exp.learning_path_item lpi join ace_api_server_oer_student_exp.learning_path lp on lpi.learning_path_id = lp.learning_path_id and lpi.version = lp.version join ace_api_server_oer_student_exp.course_lesson cl on cl.course_lesson_id = lp.course_lesson_id join centralized_content.document dc on dc.doc_seq_id=lpi.doc_seq_id join ace_api_server_oer_student_exp.course c on c.course_id=cl.course_id  where dc.doc_seq_id in (264277527,272306692,272307144,264277642)
 and lpi.deleted=0 and dc.deleted=0 and lp.deleted=0 and cl.deleted=0 and c.deleted=0 ;



