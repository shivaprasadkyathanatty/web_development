select distinct dc.doc_seq_id from ace_api_server_oer_student_exp.course c join ace_api_server_oer_student_exp.course_lesson cl on cl.course_id=c.course_id join ace_api_server_oer_student_exp.learning_path lpd on cl.course_lesson_id=lpd.course_lesson_id join ace_api_server_oer_student_exp.learning_path_item lpi on lpd.learning_path_id=lpi.learning_path_id join centralized_content.document dc on lpi.doc_seq_id=dc.doc_seq_id where c.deleted=0 and dc.deleted=0 and lpi.deleted=0 and lp.deleted=0 and cl.deleted=0 and c.course_id in (8182,8011,8022,8181) limit 1;




