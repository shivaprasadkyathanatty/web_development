select c.course_id,c.course_name,cast(AES_DECRYPT(au.first_name,'7ZHsfXDRb7NIKv0jYpRmUO9G2X5HRnyrbP944uM5h') as CHAR(10000) character set utf8) first_name,cast(AES_DECRYPT(au.last_name,'7ZHsfXDRb7NIKv0jYpRmUO9G2X5HRnyrbP944uM5h') as CHAR(10000) character set utf8) last_name,cast(AES_DECRYPT(au.email,'7ZHsfXDRb7NIKv0jYpRmUO9G2X5HRnyrbP944uM5h') as CHAR(10000) character set utf8) email from course c join arp_user au on c.user_id=au.user_id where c.global_course_source_id is null and c.deleted=0;



