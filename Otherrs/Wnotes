CREATE TABLE `jstor` (
  `document_id` char(32) NOT NULL,
  `num_pages` int(11) DEFAULT NULL,
  `document_type_id` smallint(6) NOT NULL,
  `title` varchar(384) DEFAULT NULL,
  `doc_seq_id` int(11) NOT NULL,
  `description` varchar(512) DEFAULT NULL,
  `ctime` bigint(20) NOT NULL,
  `author` varchar(256) DEFAULT NULL,
  `licensing` varchar(64) DEFAULT NULL,
  `num_seconds` int(11) DEFAULT NULL,
  `publish_date` bigint(20) DEFAULT NULL,
  `source_id` int(11) DEFAULT NULL,
  `is_tpi_url` tinyint(1) DEFAULT '0',
  `textbook_toc_id` int(11) DEFAULT NULL,
  `textbook_id` int(11) DEFAULT NULL,
  `abstract` blob,
  `issue` varchar(200) DEFAULT NULL,
  `subsource_id` int(11) DEFAULT NULL,
  `journal` varchar(256) DEFAULT NULL,
  `volume` varchar(16) DEFAULT NULL,
  `publisher` varchar(256) DEFAULT NULL,
  `owner_id` int(11) DEFAULT NULL,
  `institute_code` varchar(255) DEFAULT NULL,
  `mtime` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `document_url` varchar(1024) DEFAULT NULL,
  `has_iframe_buster` smallint(1) DEFAULT NULL,
  `known_accessibility_problems` smallint(6) unsigned DEFAULT NULL,
  `likely_accessibility_problems` smallint(6) unsigned DEFAULT NULL,
  `potential_accessibility_problems` smallint(6) unsigned DEFAULT NULL,
  `closed_caption_available` tinyint(1) NOT NULL DEFAULT '0',
  PRIMARY KEY (`doc_seq_id`),
  KEY `document_type_id` (`document_type_id`),
  KEY `source_id` (`source_id`),
  KEY `textbook_id` (`textbook_id`),
  KEY `textbook_toc_id` (`textbook_toc_id`),
  KEY `document_id` (`document_id`),
  KEY `subsource_id` (`subsource_id`),
  KEY `owner_id` (`owner_id`,`institute_code`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8


python solr_index_automation.py --bucketname emraug11 --outputdir dump_aug11 --main-dbhost se-db.prod-se.aws.learningace.com --document-text-dbhost document-text-prod.cxen17t6uhqr.us-east-1.rds.amazonaws.com --solr-url http://10.54.198.29:8983/solr/emr-all-20180912


use ace_api_server_sbvc_student_exp;
drop table if exists library_status_tmp;
CREATE TABLE `library_status_tmp` (
  `document_type_id` int(11) NOT NULL,
  `source_id` int(11) NOT NULL,
  `count` int(11) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

use centralized_content;

insert into ace_api_server_sbvc_student_exp.library_status_tmp select d.document_type_id,d.source_id,count(doc_seq_id) from document d join ace_api_server_sbvc_student_exp.source_permission sp on d.source_id=sp.source_id where d.source_id is not null group by 1,2;
insert into ace_api_server_sbvc_student_exp.library_status_tmp select d.document_type_id,d.source_id,count(doc_seq_id) from document d join ace_api_server_sbvc_student_exp.subsource_permission ssp on d.subsource_id=ssp.subsource_id and d.subsource_id is not null group by 1,2;
insert into ace_api_server_sbvc_student_exp.library_status_tmp select d.document_type_id,d.source_id,count(doc_seq_id) from document d join ace_api_server_sbvc_student_exp.docseq_permission dp using(doc_seq_id) group by 1,2;

drop table if exists ace_api_server_sbvc_student_exp.library_status_old;
rename table ace_api_server_sbvc_student_exp.library_status to ace_api_server_sbvc_student_exp.library_status_old;
rename table ace_api_server_sbvc_student_exp.library_status_tmp to ace_api_server_idu_student_exp.library_status;


