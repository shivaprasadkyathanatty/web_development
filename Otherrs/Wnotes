select c.course_id,c.course_code,c.course_name,c.deleted,count(distinct dc.doc_seq_id) from ace_api_server_gcmp_student_exp.learning_path_item lpi join ace_api_server_gcmp_student_exp.learning_path lpd on lpd.learning_path_id=lpi.learning_path_id join ace_api_server_gcmp_student_exp.course_lesson cl on cl.course_lesson_id=lpd.course_lesson_id join centralized_content.document dc on lpi.doc_seq_id=dc.doc_seq_id join ace_api_server_gcmp_student_exp.course c on c.course_id=cl.course_id where dc.deleted=0 and lpi.deleted=0 and c.deleted=0 and lpd.deleted=0 and dc.source_id=9  and c.course_id in (select c.course_id from course c where c.course_code in (select c.course_code from course c join ioc_whitelisted_courses iwc on c.course_id=iwc.course_id) and deleted=0) group by c.course_id;


document type cleanse (article):
Count:
  1.  document_type_id 41 (Article): 46798262
      journal exists: 45323833
      #update centralized_content.document set document_type_id=32 where document_type_id=41 and journal is not null and deleted=0;
      journal doesnt exists: 1474429 (*:* AND NOT pub_journal:*) document_type_id:41

  2.  document_type_id 32 (Aticle/Journal): 93207848
      journal exists: 90001542
      journal doesnt exists:3206306 (*:* AND NOT pub_journal:*) document_type_id:32
      #update centralized_content.document set document_type_id=41 where document_type_id=32 and journal is null and deleted=0;

Post query:
  1.  document_type_id 41 (Article): 46798262
      journal exists: NULL
      journal doesnt exists:  1474429 +   3206306 = 4680735
  2. document_type_id 32 (Article/Journal):
      journal exists: 90001542 + 45323833 = 135325375
      journal doesnt exists:  NULL



journal exists for: (pub_journal:[* TO *]) = 197788935
journal exists and document is Article: (pub_journal:[* TO *] AND document_type_id:41) = 45323833
journal exists and document is Article/Journal: (pub_journal:[* TO *] AND document_type_id:32) = 90001542
journal exists but pub_journal:[* TO *] AND NOT document_type_id:32 AND NOT document_type_id:41 = 62463560




