next solr run notes:

  take backup of emr/emr.py and
  copy newemr/emr.py to emr/emr.py

  change instances to 45

  solr automation script changes:
    sudo service solr restart
    sudo -i -u solr
    cd /var/solr/data/
    sudo chown -R solr emrcore
    cd /mnt/solr/
    sudo chown -R solr emrcore

    host machine:
      sudo su
      cp /var/solr/data/emr-20190206 /mnt/instanceDir/ -r
      chown -R ubuntu:ubuntu /mnt/instanceDir/emr-20190206
      exit

    target machine:
      sudo mkdir /mnt/instanceDir/emr-20190206
      sudo chown -R ubuntu:ubuntu /mnt/instanceDir/emr-20190206
      rsync -ravh ubuntu@10.54.199.93:/mnt/instanceDir/emr-20190206/* /mnt/instanceDir/emr-20190206/
      sudo rm -rf /mnt/instanceDir/emr-20190206/core.properties
      sudo su
      mkdir /var/solr/data/emr-20190206
      cp /mnt/instanceDir/emr-20190206/* /var/solr/data/emr-20190206/ -r
      chown -R solr:root /var/solr/data/emr-20190206

      sudo mkdir /mnt/solr/emr-20190206
      sudo chown -R ubuntu:ubuntu /mnt/solr/emr-20190206
      rsync -ravh ubuntu@10.54.199.93:/mnt/solr/emr-20190206/* /mnt/solr/emr-20190206/
      sudo chown -R solr:root /mnt/solr/emr-20190206
      exit



